/**
 * PREDICTOR + RECOMENDADOR M&P - SISTEMA CORE
 * Especificación: 10 variables core, Índices IP/ID, 3 escenarios, 12 meses
 */

export interface MPCoreVariables {
  // CORE VARIABLES (11 con industria)
  presupuesto_mensual: number                    // CLP - 6 tramos
  ticket_promedio: number                        // CLP - 4 tramos
  objetivo: 'BRANDING' | 'CONVERSION'            // Simple binario
  tipo_cliente: 'B2B' | 'B2C' | 'MIXTO'         // 3 opciones
  industria: 'ECOMMERCE' | 'TECNOLOGIA_SAAS' | 'SERVICIOS_PROFESIONALES' | 'SALUD_MEDICINA' | 'EDUCACION_ONLINE' | 'INMOBILIARIA' | 'AUTOMOTRIZ' | 'FINTECH' | 'MODA_RETAIL' | 'TURISMO' | 'CONSTRUCCION_REMODELACION' | 'VETERINARIA_MASCOTAS' | 'DEPORTES_FITNESS' | 'GASTRONOMIA_DELIVERY' | 'SEGUROS' // 15 industrias
  ciclo_venta: 'INSTANTANEO' | 'MENOS_1_MES' | 'UNO_A_TRES_MESES' | 'MAS_3_MESES'
  competencia_publicitaria: number              // Índice 0-10
  antiguedad_campanas: '0_3M' | '3_6M' | '6_12M' | 'MAS_12M'
  margen_bruto: number                          // Porcentaje: <30 | 30-50 | 50-70 | >70
  tasa_cierre_comercial: number                 // Porcentaje: <5 | 5-10 | 10-20 | >20
  geo_cluster: 'SANTIAGO' | 'NORTE' | 'CENTRO' | 'SUR' | 'AUSTRAL'
}

export interface MPAvanzadasVariables {
  // AVANZADAS (opcionales, activables según caso)
  ltv_ticket_ratio: number                      // <1.5 | 1.5-3 | 3-5 | >5
  calidad_creativa: number                      // Índice 0-10
  calidad_landing: number                       // Índice 0-10
  maturidad_marca: 'BAJA' | 'MEDIA' | 'ALTA'
  madurez_datos_crm: 'INEXISTENTE' | 'BASICA' | 'COMPLETA'
  capacidad_operativa: 'BAJA' | 'MEDIA' | 'ALTA'
  restricciones_legales: 'SIN' | 'MODERADAS' | 'ALTAS'
  historico_inversion_12m: 'NULO' | 'BAJO' | 'MEDIO' | 'ALTO'
  saturacion_audiencia: number                  // Índice 0-10
  mix_organico_paid: number                     // % orgánico estimado
}

export interface MPInputCompleto extends MPCoreVariables {
  avanzadas?: Partial<MPAvanzadasVariables>
}

